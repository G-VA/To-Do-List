<div class="container">
        <div class="py-4">
            <h2 class="mb-3">Personal To-Do List</h2>
        <div class="list-group">
        <% for (let i = 0; i < personalList.length; i++) { %>
            <label class="list-group-item d-flex gap-3 fade-on-check">
                <input class="form-check-input flex-shrink-0" type="checkbox" value=""  style="font-size: 1.375em;">
                <span class="pt-1 form-checked-content">
                  <strong class="white"><%= personalList[i].nTask %></strong>
                  <small class="d-block text-body-secondary">
                    <%= personalList[i].sTime %> â€“ <%= personalList[i].eTime %>
                  </small>
                </span>
              </label>

        <% } %>
        </div>
    </div>
</div>
    

